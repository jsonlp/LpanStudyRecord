<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/gray"
                android:paddingTop="100dp"
                android:orientation="vertical">


    <ImageView
    android:id="@+id/fab"
    android:layout_width="40dp"
    android:background="@drawable/circle_gray_5dp"
    android:layout_height="40dp"
    />
    <com.lpan.study.view.pathanimation.PaperPlaneView
        android:id="@+id/paper_plane1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        android:src="@drawable/download"/>

    <com.lpan.study.view.pathanimation.PaperPlaneView
        android:id="@+id/paper_plane2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        android:src="@drawable/download"/>

    <com.lpan.study.view.BrokenLineView
        android:id="@+id/broken_line_view"
        android:layout_width="10dp"
        android:background="@color/white"
        android:layout_height="100dp"/>

</RelativeLayout>