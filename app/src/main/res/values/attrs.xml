<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="card">
        <attr name="itemMarginTop" format="dimension" />
        <attr name="bottomMarginTop" format="dimension" />
        <attr name="yOffsetStep" format="dimension" />
        <attr name="widthHeightRate" format="float" />
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>
